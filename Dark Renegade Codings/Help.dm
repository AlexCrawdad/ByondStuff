mob/Topic(href,href_list[])
	switch(href_list["action"])
		if("Whisper")
			var/message = input("What would you like to reply?","Whisper") as null|text
			if(!message)return
			if(GMLevel>=1)
				src<<"<font color = #F819F5>(GM) <font color = blue><a href=?src=\ref[usr];action=Whisper>[usr]</a></font> Whispers: [message]"
				usr<<"<font color = purple>To [src]: [message]"
			else
				if(src.gooc_on)
					src<<"<font color = #F819F5><font color = blue><a href=?src=\ref[usr];action=Whisper>[usr]</a></font> Whispers: [message]"
					usr<<"<font color = purple>To [src]: [message]"
		if("Main")mainfile()
		if("Guide")guidefile()
		if("TransFile")transfile()
		if("TechFile")techsfile()
		if("Rules")rulesfile()
		if("Changelog")Changelog()
		if("Credits")Credits()
		if("Ranks")Ranks()
		if("Quests")questfile()
		if("Boosts(NEW)")Boostfile()
		if("EarthAreaSystem")Areafile()
		if("SmileyGuide")Smileyfile()
		if("MasteryGuide")masteryfile()
	.=..()
